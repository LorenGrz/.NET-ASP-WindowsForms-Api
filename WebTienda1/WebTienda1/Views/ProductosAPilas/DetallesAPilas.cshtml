@{
    ViewData["Title"] = "Detalles";
}

@model ProyectoCompartido.ProductoAPilas;

<h1>Detalle de Producto a Pilas</h1>

<div>
    <h6>Id: @Model.Id</h6>
    <h6>Nombre: @Model.Nombre</h6>
    <h6>Precio: @Model.Precio</h6>
    <h6>Cantidad: @Model.Cantidad</h6>
    <h6>Cantidad de Pilas: @Model.CantPilas</h6>
    <h6>Tipo de Pilas: @Model.Tipo</h6>
    <h6>Descripcion: @Model.Descripcion</h6>
</div>

<form asp-action="Index" method="get">
    <button type="submit" class="btn btn-success">Volver</button>
</form>